<%- include('partials/header'); -%>

<body>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label>Title</label>
            <input class="form-control" type="text" name="postTitle" placeholder="" autocomplete="off"/>
            <label>Post</label>
            <textarea class="form-control" name="postContent" rows="5" cols="149"></textarea>
        </div>
        <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>
</body>

<%- include('partials/footer'); -%>